                                    <![CDATA[import datetime
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        #~ puts code for box initialization here
        pass

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onData(self, p): # pは起床予定時刻
        CurrentTime=datetime.datetime.now()
        hour=CurrentTime.hour
        minute=CurrentTime.minute
        second=CurrentTime.second
        param=[hour,minute,second]# 現在時刻
#        self.logger.info("paramの中身は%sだよ"%param)
#        self.logger.info("pの中身は%sだよ"%p)
        want_time=int(p[0])*60*60+int(p[1])*60+int(p[2])
        now_time=param[0]*60*60+param[1]*60+param[2]
        try:
            want_time = float(want_time)
            now_time = float(now_time)
        except:
            want_time = str(want_time)
            now_time = str(now_time)
        operator = self.getParameter("Condition operator")
#        self.logger.info("起きたい時間（秒）：%s"%want_time)
#        self.logger.info("今の時間（秒）：%s"%now_time)
        if want_time > now_time:
#            self.logger.info("はやい")
            self.output_then("はやい")
        else:
#            self.logger.info("おそい")
            self.output_else("おそい")]]>
